{
  "entrypoint": "index.html",
  "shell": "src/bingo-app.html",
  "fragments": [
    "src/bingo-menu.html",
    "src/bingo-couleurs.html",
    "src/bingo-immatriculations.html",
    "src/bingo-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "x-builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ],
  "builds": [
    {
      "preset": "es6-unbundled"
    }
  ]  
}
